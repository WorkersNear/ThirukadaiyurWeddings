<script>


    $(document).ready(function () {

       
       

        $("#msgform").validate({
            rules: {
                ignore: ":not(:visible)",
            
                Mobile: {
                    required: true
                }              

            },
            messages: {
                Mobile: {
                    required: '* Mobile number is required',
                }
               
            }
        });
    });
    function sendmsg(e) {

        if ($('#msgform').valid()) {
            $('#loader').show();

            var msg = new Object();
            msg.Name = $("#name").val();           
            msg.MobileNumber = $('#Mobile').val();         
           
            $.ajax({
                type: "POST",
                url: '@Url.Action("Enquiry", "Home")',
                data: JSON.stringify(msg),
                data: msg,
                success: function (response) {
                    if (response != null) {
                        $('#loader').hide();
                      
                    } else {
                        $('#loader').hide();
                    }
                },
                failure: function (response) {
                    $('#loader').hide();
                },
                error: function (response) {
                    $('#loader').hide();
                }
            });
        }
        else {
            $('#Mobile').focus();
        }
    }
</script>
















